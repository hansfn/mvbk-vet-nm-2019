{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "desandro/imagesloaded": "^4.1",
        "desandro/masonry": "^4.1",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/admin_toolbar": "^1.24",
        "drupal/auto_entitylabel": "~2.0",
        "drupal/bootstrap": "~3.0",
        "drupal/bootstrap_layouts": "~5.0",
        "drupal/coffee": "^1.0",
        "drupal/commerce": "~2.0",
        "drupal/console": "^1.8.0",
        "drupal/core": "^8.6",
        "drupal/ctools": "~3.0",
        "drupal/devel": "^1.0",
        "drupal/ds": "^3.0",
        "drupal/entity": "^1.0@beta",
        "drupal/file_browser": "~1.0",
        "drupal/inline_entity_form": "~1.0",
        "drupal/masquerade": "^2.0@beta",
        "drupal/media_entity_image_exif": "1.x-dev",
        "drupal/mimemail": "1.x-dev",
        "drupal/pathauto": "^1.3",
        "drupal/prepopulate": "^2.0@alpha",
        "drupal/rules": "^3.0@alpha",
        "drupal/search_api": "^1.0@beta",
        "drupal/token": "~1.0",
        "drupal/transliterate": "^1.3",
        "drupal/views_send": "^1.0",
        "drush/drush": "^9.0.0",
        "enyo/dropzone": "^5.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.6"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        },
        "patches": {
            "drupal/core": {
                "Add Auto orientation image effect": "https://www.drupal.org/files/issues/2664632-17.patch"
            }
        }
    }
}
